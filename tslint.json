{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-security"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single",
            "avoid-escape"
          ],
          "semicolon": true,                
          "no-console": [
            true,
            "debug",
            "info",
            "time",
            "timeEnd",
            "trace"
          ],          
    },
    "rulesDirectory": []
}